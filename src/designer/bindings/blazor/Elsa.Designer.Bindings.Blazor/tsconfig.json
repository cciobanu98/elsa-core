{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "allowUnreachableCode": true,
        "allowUnusedLabels": true,
        "declaration": false,
        "experimentalDecorators": true,
        "lib": [
            "dom",
            "es2019"
        ],
        "moduleResolution": "node",
        "module": "esnext",
        "target": "ES2019",
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "jsx": "react",
        "jsxFactory": "h"
    }
}